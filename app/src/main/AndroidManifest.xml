<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anuj.RecordTask">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/calendar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/calendar"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.anuj.RecordTask.CompletedActivity">
        </activity>
        <activity android:name="com.anuj.RecordTask.ListActivity" />
        <activity
            android:name="com.anuj.RecordTask.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>