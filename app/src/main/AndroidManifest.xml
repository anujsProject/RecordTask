<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anuj.RecordTask">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/calendar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/calendar"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AllTask"></activity>
        <activity android:name=".CompletedActivity"></activity>
        <activity android:name=".ListActivity" />

        <receiver android:name=".ui.RemindTask" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".ui.RemindTask" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <receiver android:name=".ui.RemindTask" />
    </application>

</manifest>